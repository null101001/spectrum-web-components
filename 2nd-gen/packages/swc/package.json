{
  "name": "@adobe/swc",
  "version": "0.0.4",
  "private": true,
  "description": "Spectrum Web Components 2nd generation - rendering implementations",
  "license": "Apache-2.0",
  "author": "Adobe",
  "homepage": "https://opensource.adobe.com/spectrum-web-components/",
  "repository": {
    "directory": "2nd-gen/packages/swc",
    "type": "git",
    "url": "https://github.com/adobe/spectrum-web-components.git"
  },
  "bugs": {
    "url": "https://github.com/adobe/spectrum-web-components/issues"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/components/*/index.d.ts",
      "import": "./dist/components/*/index.js"
    },
    "./components/*": {
      "types": "./dist/components/*/index.d.ts",
      "import": "./dist/components/*/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "analyze": "cem analyze --config cem.config.js",
    "analyze:watch": "cem analyze --config cem.config.js --watch",
    "build": "vite build",
    "clean": "rimraf dist",
    "dev": "vite build --watch",
    "dev:core": "yarn workspace @spectrum-web-components/core dev",
    "storybook": "yarn analyze && storybook dev -p 6006",
    "storybook:build": "yarn analyze && storybook build",
    "stylesheets": "swc-tokens --outputType stylesheet --out ./stylesheets/tokens.css --prefix swc",
    "test": "vitest --run --project storybook",
    "test:browser": "vitest --project storybook --browser.headless=false",
    "test:coverage": "vitest --coverage --project storybook",
    "test:ui": "vitest --ui --project storybook",
    "test:watch": "run-p dev:core test:watch:vitest",
    "test:watch:vitest": "vitest --project storybook --watch"
  },
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@spectrum-web-components/core": "0.0.4",
    "lit": "^2.5.0 || ^3.1.3"
  },
  "devDependencies": {
    "@adobe/postcss-token": "workspace:*",
    "@adobe/swc-tokens": "workspace:*",
    "@axe-core/playwright": "4.10.2",
    "@custom-elements-manifest/analyzer": "0.10.8",
    "@storybook/addon-a11y": "10.2.8",
    "@storybook/addon-designs": "11.1.2",
    "@storybook/addon-docs": "10.2.8",
    "@storybook/addon-vitest": "10.2.8",
    "@storybook/test-runner": "0.24.2",
    "@storybook/web-components-vite": "10.2.8",
    "@types/react": "19.1.16",
    "@types/react-dom": "19.1.9",
    "@vitest/browser": "4.0.15",
    "@vitest/browser-playwright": "4.0.15",
    "@vitest/coverage-v8": "4.0.15",
    "@vitest/ui": "4.0.15",
    "@wc-toolkit/storybook-helpers": "10.0.0",
    "autoprefixer": "10.4.21",
    "glob": "11.0.3",
    "playwright": "1.53.1",
    "postcss": "8.5.6",
    "postcss-preset-env": "10.4.0",
    "prettier": "^3.7.4",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "remark-gfm": "4.0.0",
    "rimraf": "6.0.1",
    "storybook": "patch:storybook@npm%3A10.2.8#~/.yarn/patches/storybook-npm-10.2.8.patch",
    "typescript": "5.9.2",
    "vite": "7.1.7",
    "vite-plugin-dts": "4.5.4",
    "vite-plugin-lit-css": "2.2.1",
    "vitest": "4.0.15"
  },
  "keywords": [
    "design-system",
    "spectrum",
    "adobe",
    "adobe-spectrum",
    "web components",
    "web-components",
    "lit-element",
    "lit-html",
    "component",
    "css"
  ],
  "customElements": ".storybook/custom-elements.json",
  "publishConfig": {
    "access": "public"
  }
}
